{
    "family": "leket-israel-frontend",
    "containerDefinitions": [
        {
            "name": "frontend",
            "image": "152203247831.dkr.ecr.us-east-1.amazonaws.com/leket-israel-frontend:leket-israel-frontend-5241536703",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "frontend-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
			  {
				"name": "VITE_BASE_API_URL",
				"value": "http://leket-israel-backend-lb-180912419.us-east-1.elb.amazonaws.com:3000"
			  }
			],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/leket-israel-frontend",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::152203247831:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::152203247831:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
